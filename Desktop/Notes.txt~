 public function leaf_category_path(){
        $leaf_category_sql_result= array_values($this->get_leaf_categories());
        foreach($leaf_category_sql_result as $key=>$value){
                $category_path['category_path'][$key]['category_id'] = $value['category_id'];
                $category_path['category_path'][$key]['category_path'] = str_replace("///"," >> ",str_replace(",","-",$value['category_path']));
        //      $category_path['category_path'][$key]['meta_keywords'] = str_replace(",","-",$value['meta_keywords']);
        }
        $this->response($this->json($category_path), 200);
     }



